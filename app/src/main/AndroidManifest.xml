<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toussurlesjeux">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_quintero"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ModifierJeuActivity"></activity>
        <activity android:name=".FicheJeuActivity" />
        <activity android:name=".AjouterJeuActivity" />
        <activity
            android:name=".ListeJeuxActivity"
            android:label="@string/title_activity_liste_jeux"
            android:theme="@style/NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>